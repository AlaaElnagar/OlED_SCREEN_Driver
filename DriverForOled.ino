void OLED_vIdSendCMD(u8 Copy_u8CMD){
/*//SENDslv_add with write i2c_SEND OLED SLAVE ADDREESS IN I2CSED_SLV_ADDRESS(OLED_I2C_ADR)*
 *i2c_SENDDATA(OLED_CMD_REG_ADR()    //GO TO CMD REGISTER
 *I2C_SENDDAT(Copy_u8CMD)
 *STOP COMMUNICATION
 */
 Wire.beginTransmission(OLED_I2C_ADR) ;
 Wire.write(OLED_CMD_REG_ADR);
 Wire.write(Copy_u8CMD);
 Wire.endTransmission();
}

void OLED_vIdSendData(u8 Copy_u8Data){
/*//SENDslv_add with write i2c_SEND OLED SLAVE ADDREESS IN I2CSED_SLV_ADDRESS(OLED_I2C_ADR)*
 *i2c_SENDDATA(OLED_DATA_REG_ADR()    //GO TO CMD REGISTER
 *I2C_SENDDAT(Copy_u8Data)
 *STOP COMMUNICATION
 */

  Wire.beginTransmission(OLED_I2C_ADR) ;
 Wire.write(OLED_DATA_REG_ADR);
 Wire.write(Copy_u8Data);
 Wire.endTransmission();
}

void OLED_vIdTurnOn(){
  OLED_vIdSendCMD(OLED_ON_CMD);
}

void OLED_vIdSleepMode(){
  OLED_vIdSendCMD(OLED_OFF_CMD);
}

void OLED_vIdNormlDispMode(){
  OLED_vIdSendCMD(OLED_NORMAL_DISP_CMD_RST);
}

void OLED_vIdInverseDispMode(){
  OLED_vIdSendCMD(OLED_INVERS_DISP_CMD);
}

void OLED_vIdSetPageMode(){
  OLED_vIdSendCMD(0x20);                       /*set addressing mode*/
  OLED_vIdSendCMD(OLED_PAGE_ADDR_MODE);        /*Set page addressign mode*/
}

/*Very Important function to access any column in the screen OF 8 PIXELS
 * X  >> 0 : 127 COULMN
 * Y  >> 0 : 7   PAGE
 * */

void OLED_vIdSetCurser(u8 Copy_u8ColumnX , u8 Copy_u8PageY  )
{
  /*Send Column First FOUR Nible  Lower Address*/
  OLED_vIdSendCMD( 0X00 + ( Copy_u8ColumnX & 0X0F       ));
  /*Send Column First FOUR Nible  Higher  Address*/
  OLED_vIdSendCMD( 0X10 + ((Copy_u8ColumnX >> 4 ) & 0X0F));

  OLED_vIdSendCMD( 0XB0 + ( Copy_u8PageY                ));


}





void OLED_vIdClearFullDisplay(){

  for (u8 page= 0 ; page <8 ; page++){
    OLED_vIdSetCurser(0,page);
    for (u8 j = 0 ; j < 127 ; j ++){
      /*auto coulmn increment*/
      OLED_vIdSendData(0x00) ;

    }
  }

  OLED_vIdSetCurser(0,0);

}


void OLED_vIdInit(){
  
/*start i2c */
Wire.begin();
/*Oled Intialization synario*/
  OLED_vIdTurnOn();
  OLED_vIdNormlDispMode();
  OLED_vIdSetPageMode();
  /*There is internal hardware in display Chargepump*/
  /*Charge Pump Regulator
The internal regulator circuit in SSD1306 accompanying only 2 external capacitors can generate a
7.5V voltage supply, VCC, from a low voltage supply input, VBAT. The VCC is the voltage supply to the
OLED driver block. This is a switching capacitor regulator circuit, designed for handheld applications.
This regulator can be turned on/off by software command setting.*/

    OLED_vIdSendCMD(0x8d);
  OLED_vIdSendCMD(0x14);
  OLED_vIdClearFullDisplay();

}


