for (u8 j = Copy_u8ColumnXPOS  ; j <(Copy_u8ColumnXPOS+Columns) ; j++  ){
  
  for (u8 i = Copy_u8RawYPOS ; i < (Copy_u8RawYPOS+ Pages) ; i++){

     OLED_vIdSetCurser(j,i);
